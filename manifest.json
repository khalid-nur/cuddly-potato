{
  "manifest_version": 3,
  "name": "Prayer-Time",
  "version": "1.0.0",
  "description": "When extension is open it launch and display prayer time",

  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["contextMenus", "tabs", "geolocation"],

  "action": {
    "default_popup": "popup.html"
  }
}
